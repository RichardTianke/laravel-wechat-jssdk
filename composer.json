{
  "name": "tinfot/laravel-wechat-jssdk",
  "type": "library",
  "description": "Laravel Wechat JS-SDK",
  "homepage": "https://github.com/tinfot/laravel-wechat-jssdk",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "laravel",
    "wechat",
    "jssdk",
    "laravel-wechat-jssdk"
  ],
  "authors": [
    {
      "name": "Richard Tian",
      "email": "richard_tianke@qq.com",
      "homepage": "http://richard.lol",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/tinfot/laravel-wechat-jssdk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tinfot\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.4.*",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.8",
    "ext-curl": "*"
  }
}